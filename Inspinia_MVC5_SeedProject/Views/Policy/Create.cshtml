@model Inspinia_MVC5_SeedProject.Models.PolicyEntityBaseViewModel
@using Inspinia_MVC5_SeedProject.App_LocalResources

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title" id="myModalLabel">@Resource.PolicyCreateTitle</h4>
</div>

<div class="modal-body">
    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()
        <div class="form-horizontal">
            @Html.ValidationSummary(true)
            

            @Html.Partial("CreateWizard", Model)
           
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">@Resource.CancelBtn</button>
            <button id="createPolicyBtn" type="submit" class="btn btn-primary">@Resource.SaveBtn</button>
        </div>
    }
</div>

@Scripts.Render("~/bundles/jqueryval")
@Scripts.Render("~/bundles/jqueryValUnobt")
